    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typedoc-plugin-markdown": "^3.13.4",
